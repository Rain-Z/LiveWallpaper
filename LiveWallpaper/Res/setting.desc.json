      "type": "combobox",
      "lanKey": "setting_displayMonitor",
      "descLanKey": "setting_displayMonitor_desc",
      "cbItems": "$DisplayMonitor"
    },
    "VideoAspect": {
      "type": "string",
      "lanKey": "setting_videoAspect",
      "descLanKey": "setting_videoAspect_desc"
    }
